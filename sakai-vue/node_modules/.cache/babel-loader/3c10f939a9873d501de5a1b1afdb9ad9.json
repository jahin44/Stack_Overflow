th);\r\n\t\t\t\t\tthis.sortKey = sortValue;\r\n\t\t\t\t}\r\n\t\t\t\telse {\r\n\t\t\t\t\tthis.sortOrder = 1;\r\n\t\t\t\t\tthis.sortField = value;\r\n\t\t\t\t\tthis.sortKey = sortValue;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n@import '../assets/demo/badges.scss';\r\n</style>\r\n"]}]}